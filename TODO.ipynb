    "query = \"\"\"create OR REPLACE function finalmes ()\n",
    "       RETURNS table (cliente VARCHAR, punticos NUMERIC) as \n",
    "    $$\n",
    "        DECLARE\n",
    "        \n",
    "        hoy TIMESTAMP:= date_part('month',now());\n",
    "        \n",
    "        BEGIN       \n",
    "             \n",
